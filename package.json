  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChepteaCatalin/transdurex/issues"
  },
  "homepage": "https://github.com/ChepteaCatalin/transdurex#readme",
  "devDependencies": {
    "@vitest/coverage-v8": "^2.0.5",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  }
